<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    tools:context=".Activity.AfterAddingServiceActivity">

    <ImageView
        android:layout_width="181dp"
        android:src="@drawable/addingsucces"
        android:layout_marginTop="100dp"
        android:layout_height="161dp">
    </ImageView>
    <TextView
        android:id="@+id/Tvadvertisawahats"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:drawablePadding="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:textColor="@color/Black"
        android:textStyle="bold"
        android:gravity="center"
        android:text="@string/sucessadd" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:id="@+id/sucessaddtv"
        android:drawablePadding="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:textColor="@color/gray5"
        android:gravity="center"
        android:text="@string/sucessaddtv" />
    <ImageView
        android:id="@+id/gotohome"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawablePadding="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/gotohome"
        android:gravity="center"
      />
</LinearLayout>